<%-include("partials/header")-%>
<h1>Compose</h1>
<form class="post" action="/compose" method="post">
  <div class="form-group">
    <label>Title</label>
    <input type="text" class="form-control" name="postTitle" placeholder="">
  </div>
  <div class="form-group">
    <label>Post</label>
    <textarea class="form-control"  name="postBody" rows="8" cols="80"  placeholder=""></textarea>
  </div>
  <button type="submit" class="btn btn-primary">Publish</button>
</form>
<%-include("partials/footer")-%>
